#include "main.h"

/**
 * jack_bauer - affiche heure min sec
 * return - fin script
 */

void jack_bauer(void)
{
	int dh, uh, dm, um;

	for (dh = 0; dh <= 2; dh++)
	{
		for (uh = 0; uh <= 9; uh++)
		{
			if ((dh <= 1 && uh <= 9) || (dh <= 2 && uh <= 3))
			{
				for (dm = 0; dm <= 5; dm++)
				{
					for (um = 0; um <= 9; um++)
					{
						_putchar(dh + '0');
						_putchar(uh + '0');
						_putchar(58);
						_putchar(dm + '0');
						_putchar(um + '0');
					}
				}
			}
		}
	}
}
