#include "main.h"
#include <stdio.h>
/**
 * print_to_98 - affiche les nb entiers
 * @n: check nb
 * return: fin du script
 */

void print_to_98(int n)
{
	if (n <= 97)
	{
		for (; n <= 97; n++)
		{
			printf("%d, ", n);
			printf("98");
				}
	}
	else if (n >= 97)
	{
		for (; n >= 97; n--)
		{
			printf("%d, ", n);
			printf("98");
				}
	}
	else
	{
		printf("98");
	}
}
